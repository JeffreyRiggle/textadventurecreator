<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
	<GridPane fx:id="pane" 
			  AnchorPane.topAnchor="0.0" 
			  AnchorPane.leftAnchor="0.0" 
			  AnchorPane.rightAnchor="0.0"
			  AnchorPane.bottomAnchor="0.0">
			<columnConstraints>
        		<ColumnConstraints hgrow="ALWAYS" prefWidth="150"/>
        		<ColumnConstraints hgrow="ALWAYS" prefWidth="150"/>
    		</columnConstraints>
    		<rowConstraints>
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="NEVER" />
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="NEVER" />
    			<RowConstraints minHeight="0" prefHeight="0" maxHeight="0" vgrow="NEVER" />
    			<RowConstraints minHeight="0" prefHeight="0" maxHeight="0" vgrow="NEVER" />
    			<RowConstraints minHeight="0" prefHeight="0" maxHeight="0" vgrow="NEVER" />
    			<RowConstraints minHeight="0" prefHeight="0" maxHeight="0" vgrow="NEVER" />
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="NEVER" />
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="NEVER" />
    		</rowConstraints>
    		<Label fx:id="playerLabel" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
			<ComboBox fx:id="player" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    		<Label fx:id="propertyLabel" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
			<ComboBox fx:id="property" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
			<Label fx:id="idLabel" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
			<ComboBox fx:id="id" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
			<Label fx:id="additionalIdLabel" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
			<ComboBox fx:id="additionalId" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
			<Label fx:id="dataMemberLabel" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
			<ComboBox fx:id="dataMember" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
			<Label fx:id="additionalDataMemberLabel" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
			<ComboBox fx:id="additionalDataMember" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
			<Label fx:id="conditionLabel" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
			<ComboBox fx:id="condition" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
			<Label fx:id="expectedValueLabel" GridPane.columnIndex="0" GridPane.rowIndex="7"/>
			<TextField fx:id="expectedValue" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
	</GridPane>
</fx:root>