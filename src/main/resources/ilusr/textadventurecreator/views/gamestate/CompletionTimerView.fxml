<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
	<GridPane>
			<columnConstraints>
        		<ColumnConstraints hgrow="ALWAYS" prefWidth="150"/>
        		<ColumnConstraints hgrow="ALWAYS" prefWidth="200"/>
    		</columnConstraints>
    		<rowConstraints>
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="ALWAYS" />
    			<RowConstraints minHeight="25.0" prefHeight="50.0" vgrow="ALWAYS" />
    		</rowConstraints>
    		<Label fx:id="stateTitle" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
			<TextField fx:id="completionData" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
			<Label fx:id="timeLabel" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
			<TextField fx:id="duration" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
	</GridPane>
</fx:root>