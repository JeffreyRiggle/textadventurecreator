<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
	<GridPane>
		<columnConstraints>
       		<ColumnConstraints hgrow="ALWAYS" prefWidth="150"/>
       		<ColumnConstraints hgrow="ALWAYS" prefWidth="150"/>
    	</columnConstraints>
    	<rowConstraints>
    		<RowConstraints minHeight="25.0" prefHeight="75" vgrow="NEVER" />
    		<RowConstraints minHeight="25.0" prefHeight="75" vgrow="NEVER" />
    		<RowConstraints minHeight="25.0" prefHeight="75" vgrow="NEVER" />
    		<RowConstraints minHeight="25.0" prefHeight="75" vgrow="NEVER" />
    	</rowConstraints>
    	<padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
    	<Label fx:id="settingLabel" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2"/>
		<Label fx:id="selectedThemeLabel" GridPane.columnIndex="0" GridPane.rowIndex="1" />
		<ComboBox fx:id="themes" GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<Button fx:id="apply" GridPane.columnIndex="1" GridPane.rowIndex="2" />
	</GridPane>
</fx:root>